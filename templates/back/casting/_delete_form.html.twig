{% if is_granted('ROLE_ADMIN') %}
<form method="post" action="{{ path('back_casting_delete', {'id': casting.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ casting.id) }}">
    <button class="btn">Delete</button>
</form>
{% endif %}